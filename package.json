{
  "name": "vue-components-lib-template",
  "main": "dist/vue-components-lib-template.umd.js",
  "module": "dist/vue-components-lib-template.es.js",
  "types": "dist/entry.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/entry.d.ts",
      "import": "./dist/vue-components-lib-template.es.js",
      "require": "./dist/vue-components-lib-template.umd.js"
    },
    "./styles.css": "./dist/vue-components-lib-template.css"
  },
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "vite --mode development --open",
    "publish": "npm run build"
  },
  "peerDependencies": {
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/node": "^22.15.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vue-tsc": "^3.0.3"
  },
  "engines": {
    "node": "^22.0.0"
  }
}
